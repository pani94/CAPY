<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ale.myapplicatio.RicercaActivityFragmentListItem">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:text="TextView"
        android:layout_width="209dp"
        android:layout_height="30dp"
        android:id="@+id/RicercaActivityFragmentListItemTitolo" />

    <ImageView
        android:layout_width="101dp"
        android:layout_height="91dp"
        app:srcCompat="@drawable/common_full_open_on_phone"
        android:id="@+id/RicercaActivityFragmentListItemImmagine"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/RicercaActivityFragmentListItemOrario"
        android:layout_alignBottom="@+id/RicercaActivityFragmentListItemImmagine"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
