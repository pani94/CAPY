    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/RicercaActivityFragmentListItemIndirizzo"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="33dp"
        android:id="@+id/RicercaActivityFragmentListItemTelefono" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/RicercaActivityFragmentListItemTelefono"
        android:layout_alignRight="@+id/RicercaActivityFragmentListItemTelefono"
        android:layout_alignEnd="@+id/RicercaActivityFragmentListItemTelefono"
        android:layout_marginTop="53dp"
        android:id="@+id/RicercaActivityFragmentListItemLink" />

</RelativeLayout>
