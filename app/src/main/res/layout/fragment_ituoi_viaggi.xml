<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context="com.example.ale.myapplicatio.FragmentITuoiViaggi">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fragment_ituoi_viaggi_textview"
        android:layout_below="@+id/i_tuoi_viaggi"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="78dp" />

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/lista_viaggi"
        android:layout_below="@+id/i_tuoi_viaggi"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="39dp" />

    <TextView
        android:layout_width="373dp"
        android:layout_height="50dp"
        android:id="@+id/i_tuoi_viaggi"
        android:textSize="35sp"
        tools:textStyle="bold"
        android:textColor="#000000"
        android:text="I Miei Viaggi"
        android:textStyle="normal|bold"
        android:layout_marginTop="59dp"
        android:textAlignment="center"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:fontFamily="casual" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_business_center_black_24dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp"
        android:id="@+id/imageView6" />

</RelativeLayout>
