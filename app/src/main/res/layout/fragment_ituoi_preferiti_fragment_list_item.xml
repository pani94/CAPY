<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.ale.myapplicatio.ITuoiPreferitiFragmentListItem">

    <TextView
        android:text="TextView"
        android:layout_width="209dp"
        android:layout_height="30dp"
        android:id="@+id/fragment_ituoi_preferiti_fragment_list_item_titolo" />

    <ImageView
        android:layout_width="101dp"
        android:layout_height="91dp"
        app:srcCompat="@drawable/common_full_open_on_phone"
        android:id="@+id/fragment_ituoi_preferiti_fragment_list_item_immagine"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fragment_ituoi_preferiti_fragment_list_item_orario"
        android:layout_above="@+id/fragment_ituoi_preferiti_fragment_list_item_indirizzo"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="13dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:linksClickable="true"
        android:autoLink="web"
        android:id="@+id/fragment_ituoi_preferiti_fragment_list_item_link"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fragment_ituoi_preferiti_fragment_list_item_telefono"
        android:layout_alignBottom="@+id/fragment_ituoi_preferiti_fragment_list_item_link"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="51dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fragment_ituoi_preferiti_fragment_list_item_indirizzo"
        android:layout_marginBottom="26dp"
        android:layout_above="@+id/fragment_ituoi_preferiti_fragment_list_item_telefono"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="+"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fragment_ituoi_preferiti_fragment_list_item_bottoneaggiungi"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginBottom="113dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textAlignment="center" />

</RelativeLayout>
